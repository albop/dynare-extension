{
  "private": true,
  "name": "dolo-extension",
  "version": "0.1.0",
  "description": "dolo renderer for JupyterLab",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "jupyter",
    "jupyterlab"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prepack": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "jupyterlab": {
    "mimeExtension": true
  },
  "dependencies": {
    "@jupyterlab/rendermime-interfaces": "^1.0.3",
    "@phosphor/messaging": "^1.2.2",
    "@phosphor/widgets": "^1.5.0",
    "@jupyterlab/services": "^1.1.2"
  },
  "devDependencies": {
    "rimraf": "~2.6.2",
    "typescript": "~2.6.2"
  },
  "files": [
    "lib/*.d.ts",
    "lib/*.js",
    "style/*.*"
  ],
  "directories": {
    "lib": "lib/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyter-renderers.git"
  },
  "author": "PW",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyter-renderers/issues"
  },
  "homepage": "https://github.com/jupyterlab/jupyter-renderers"
}
